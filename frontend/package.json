{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prebuild": "rm -rf build",
    "build": "react-scripts build && cp -r build/* ../backend/public",
